<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Курсовой проект</title>
    <link href="https://fonts.googleapis.com/css?family=Coda" rel="stylesheet">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-blue.min.css" />
    <link rel=”stylesheet” href=”https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css”>
    <link rel=”stylesheet” href=”https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.7.5/css/bootstrap-select.min.css”>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.materialdesignicons.com/4.9.95/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Coda" rel="stylesheet">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class=" mdl-js-layout mdl-layout--fixed-header">
        <header class="">
            <div id="main">
                <div class="container">
                    <nav>
                        <div class="nav-fostrap">
                            <ul>
                                <li>
                                    <a asp-controller="Home" asp-action="Index">Главная страница</a>
                                </li>
                                <li>
                                    <a asp-controller="City" asp-action="Index">Города</a>
                                </li>
                                <li>
                                    <a asp-controller="Carrier" asp-action="Index">Оператор</a>
                                </li>
                                <li>
                                    <a asp-controller="Departure" asp-action="Index">Отправления</a>
                                </li>
                                <li>
                                    <a asp-controller="Departure" asp-action="CreateSpecificallyDeparture">Создать детали</a>
                                </li>
                            </ul>
                        </div>
                        <div class="nav-bg-fostrap">
                            <div class="navbar-fostrap"> <span></span> <span></span> <span></span> </div>
                            <a href="" class="title-mobile">Система администрирования работы автовокзала</a>
                        </div>
                    </nav>
                </div>

            </div>
        </header>
        <div class="content">

            @RenderBody()

        </div>

    </div>

    <footer>
        <div class="mui-container mui--text-center">
            Курсовой проект Бахаров Абдулмалик АВТ-918 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Детали варианта</a>
        </div>
    </footer>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
    <script src=”https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.7.5/js/bootstrap-select.min.js”></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
